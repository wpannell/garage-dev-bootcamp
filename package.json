{
  "license": "larry steck and jason calalang",
  "name": "garage-dev-bootcamp",
  "private": true,
  "version": "1.0.0",
  "engines": {
    "node": "8.9.4",
    "npm": "5.6.0",
    "yarn": "1.5.1"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run spec"
    }
  },
  "scripts": {
    "ng": "ng",
    "start": "concurrently --kill-others \"npm run server\" \"ng serve --environment=local\" ",
    "start-qa": "ng serve --environment=qa --port 4300",
    "server": "json-server server/api/db.json --port 8080 --watch",
    "build": "node --stack_size=4048 node_modules/.bin/ng build --prod --environment=prod",
    "lint": "ng lint",
    "test": "npm run spec",
    "prepush": "npm run lint && npm run spec",
    "spec": "rimraf .tmp && node $NODE_DEBUG_OPTION ./node_modules/.bin/mocha-webpack --opts config/mocha/mocha-webpack.opts",
    "spec:browser": "rimraf .tmp && webpack-dev-server --config config/webpack.test.browser.js --inline --progress --port 8888",
    "spec:watch": "npm run spec -- --watch",
    "spec:server": "webpack-dev-server --config config/webpack.test.browser.js --inline --progress --port 8888",
    "spec:coverage": "rimraf reports && cross-env NODE_ENV=coverage nyc --reporter=text --reporter=text-summary mocha-webpack --opts config/mocha/mocha-webpack.opts",
    "spec:reports": "rimraf reports && cross-env NODE_ENV=coverage nyc mocha-webpack --opts config/mocha/mocha-webpack.opts --reporter=xunit --reporter-options output=reports/tests/tests-report.xml",
    "pact:server": "node contracts/dev-server/pact-server.js",
    "publish:local": "node contracts/broker/publish.js https://dataprivacy-portal-pact-broker.mybluemix.net",
    "tdd": "npm run spec:watch"
  },
  "dependencies": {
    "@angular/animations": "5.2.6",
    "@angular/cdk": "5.2.2",
    "@angular/common": "5.2.6",
    "@angular/compiler": "5.2.6",
    "@angular/core": "5.2.6",
    "@angular/forms": "5.2.6",
    "@angular/http": "5.2.6",
    "@angular/platform-browser": "5.2.6",
    "@angular/platform-browser-dynamic": "5.2.6",
    "@angular/router": "5.2.6",
    "axios": "0.18.0",
    "bootstrap": "4.0.0",
    "core-js": "2.5.1",
    "hammerjs": "2.0.8",
    "jquery": "3.3.1",
    "moment": "2.20.1",
    "popper.js": "^1.14.1",
    "rxjs": "5.5.6",
    "zone.js": "0.8.19"
  },
  "devDependencies": {
    "@angular-devkit/build-optimizer": "0.4.2",
    "@angular-devkit/core": "0.4.2",
    "@angular-devkit/schematics": "0.4.2",
    "@angular/cli": "6.0.0-beta.3",
    "@angular/compiler-cli": "5.2.6",
    "@angular/language-service": "5.2.6",
    "@pact-foundation/pact-node": "6.7.3",
    "@types/hammerjs": "^2.0.35",
    "@types/merge2": "^0.3.30",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "2.2.46",
    "@types/node": "9.3.0",
    "@types/ramda": "0.25.17",
    "angular2-template-loader": "0.6.2",
    "awesome-typescript-loader": "3.4.1",
    "bluebird": "3.5.1",
    "clrscr": "1.0.4",
    "codelyzer": "4.0.1",
    "concurrently": "3.5.0",
    "cross-env": "5.1.3",
    "css-loader": "0.28.8",
    "diff-so-fancy": "1.2.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "growl": "1.10.4",
    "html-loader": "0.5.4",
    "html-webpack-plugin": "2.30.1",
    "husky": "0.14.3",
    "istanbul-instrumenter-loader": "3.0.0",
    "jsdom": "11.5.1",
    "json-server": "0.12.1",
    "mocha": "4.1.0",
    "mocha-loader": "1.1.2",
    "mocha-webpack": "1.0.1",
    "npm-run-all": "4.1.2",
    "null-loader": "0.1.1",
    "nyc": "11.4.1",
    "pact": "4.3.2",
    "protractor": "5.2.2",
    "ramda": "0.25.0",
    "ramda-adjunct": "2.5.0",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.2",
    "should": "git+https://git@github.com/shouldjs/should.js.git",
    "source-map-support": "0.5.0",
    "start-server-webpack-plugin": "2.2.0",
    "style-loader": "0.19.1",
    "testdouble": "3.2.6",
    "timekeeper": "2.0.0",
    "ts-node": "4.1.0",
    "tsconfig-paths": "2.3.0",
    "tslint": "5.9.1",
    "tsutils": "2.6.0",
    "typescript": "2.6.2",
    "uglify-js": "2.8.14",
    "web-animations-js": "2.2.5",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.11.0",
    "webpack-merge": "4.1.1",
    "webpack-node-externals": "1.6.0"
  }
}
